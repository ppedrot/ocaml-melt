let parbox = parbox (`Textwidth 1.)

let pseudocode =
  Verbatim.pseudocode
    ~keywords: [ \"let\"; \"in\" ]
    ~symbols: [
      \"->\", rightarrow;
      \"*\", times
    ]
    ~keyword_symbols: [ \"fun\", lambda ]

let body = "
Here is a pseudocode example.

{parbox "<:pseudocode:#
let square = fun x -> x * x
let id x = x
let indented x =
  fun y ->
    x * y
let funny = indented x y
#>"}
"

let () = emit (document ~title: "Latex.Verbatim.pseudocode" body)

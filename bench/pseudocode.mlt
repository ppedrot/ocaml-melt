let parbox = parbox (`Textwidth 1.)

let pseudocode =
  Verbatim.pseudocode
    [\"let\"; \"in\"; \"fun\"]
    [\"->\", rightarrow;
     \"*\", times]

let body = "
Here is a pseudocode example.

{parbox "<:pseudocode:#let square = fun x -> x * x
let id x = x
let indented x =
  fun y ->
    x * y
let funny = indented x y
#>"}
"

let () = emit (document ~title: "Latex.Verbatim.pseudocode" body)
